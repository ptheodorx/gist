{
	"laravel comment": {
		"prefix": "lcom",
		"body": [
			"/*",
			"|--------------------------------------------------------------------------",
			"| $1",
			"|--------------------------------------------------------------------------",
			"|",
			"| $2",
			"|",
			"*/"
		],
		"description": "new Laravel style comment"
	},
	"method": {
		"prefix": "met",
		"body": [
			"public function $1()",
			"{",
			"    $2",
			"}"
		],
		"description": "public method"
	},
	"private method": {
		"prefix": "pmet",
		"body": [
			"private function $1()",
			"{",
			"    $2",
			"}"
		],
		"description": "private method"
	},
	"protected method": {
		"prefix": "prmet",
		"body": [
			"protected function $1()",
			"{",
			"    $2",
			"}"
		],
		"description": "protected method"
	},
	"log": {
		"prefix": "''",
		"body": [
			"\\Log::info($1);"
		],
		"description": "add log entry"
	},
	"memory usage": {
		"prefix": "memory",
		"body": [
			"\\Log::info('Memory Usage: '.round(memory_get_peak_usage() / 1048576, 2).'MB');"
		],
		"description": "log memory usage"
	},
	"test adnotation": {
		"prefix": "test",
		"body": [
			"/** @test */"
		],
		"description": "test adnotation for php unit"
	},
	"class": {
		"prefix": "class",
		"body": [
			"class $TM_FILENAME_BASE",
			"{",
			"    $1",
			"}"
		],
		"description": "create new class"
	},
	"construct": {
		"prefix": "construct",
		"body": [
			"public function __construct()",
			"{",
			"    $1",
			"}"
		],
		"description": "add class constructor"
	}
}